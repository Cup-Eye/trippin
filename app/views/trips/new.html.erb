<p class="back_"><%= link_to "Back", trips_path %></p>
<h1> Create a new Trip: </h1>
<%= form_for(@trip) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :destination%>
    <%= f.text_field :destination, class: 'form-control'%>
  </div>

  <%= f.submit%>
<% end %>
