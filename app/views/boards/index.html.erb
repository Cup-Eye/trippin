<p><%= link_to "Back", trips_path %></p>
<% @boards.each do |board| %>
  <%if board.type == "TimeframeBoard"
      link = time_frames_path
    elsif board.type == "AccommodationBoard"
      link = trip_accommodations_path
    elsif board.type == "DestinationBoard"
      link = trip_destinations_path
    elsif board.type == "TransportationBoard"
      link = trip_transportations_path
    end%>
  <p><%= board.title, link %></p>
<% end %>

